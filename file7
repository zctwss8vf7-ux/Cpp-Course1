#include <iostream>

using namespace std;

int main()
{
	int b;
	cout << "Podaj wynik z testu" << endl;
	cin >> b;
	if (b >= 90)
	{
		cout << "Ocena celujaca" << endl;
	}
	else if (b >= 75 && b <= 89)
	{
		cout << "Ocena bardzo dobra" << endl;
	}
	else if (b >= 50 && b <= 74)
	{
		cout << "Ocena dobra" << endl;
	}
	else if (b < 50)
	{
		cout << "Ocena niedostateczna" << endl;
	}
}
