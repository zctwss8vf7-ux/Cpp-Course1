#include <iostream>

using namespace std;

int main()
{
	double a, b, c;
	cout << "podaj trzy liczby" << endl;
	cin >> a >> b >> c;
	if (a >= b && a >= c)
	{
		cout << "Najwieksza liczba - " << a;
	}
	else if (b >= a && b >= c)
	{
		cout << "Najwieksza liczba - " << b;
	}
	else if (c >= a && c >= b)
	{
		cout << "Najwieksza liczba - " << c;
	}
}
